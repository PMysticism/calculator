<https://doi.org/10.1016/j.ijheatmasstransfer.2023.124817> a cs:ColdSprayPaper ;
    cs:hasDOI "10.1016/j.ijheatmasstransfer.2023.124817" ;

    # 1. Paper Metadata
    cs:hasMetadata [
        a cs:Metadata ;
        cs:hasTitle "A novel cold spray process flow technique - a numerical investigation" ;
        cs:hasAuthor "Kamil Kowalski, Przemysław Błasiak, Sławomir Pietrowicz" ;
        cs:hasJournal "International Journal of Heat and Mass Transfer" ;
        cs:hasPublicationYear "2024"^^xsd:gYear
    ] ;

    # 2. Computational Study Details
    cs:hasComputationalStudy [
        a cs:ComputationalStudy ;
        cs:usesSoftware "Ansys Fluent" ;
        cs:modelsMaterial "Copper (Particle)" ;
        cs:modelsMaterial "Air (Working Gas)" ;
        cs:modelsMaterial "Aluminum (Substrate)" ;

        # 2.1 Numerical Approach
        cs:hasNumericalApproach [
            a cs:NumericalApproach ;
            cs:approachType "Computational Fluid Dynamics (CFD); RNG k-epsilon turbulence model; Discrete Phase Model (DPM)" ;
            cs:dimensionality "3D" ;
            cs:simulationFocus "Gas flow dynamics and particle deposition rates (accretion rates) for classical and novel nozzle supply methods" ;

            # 2.2 Approach Details
            cs:hasApproachDetail [
                a cs:ApproachDetails ;
                cs:fluidDynamicsModel "RNG k-epsilon turbulence model with non-equilibrium wall functions; Ideal gas law for air" ;
                cs:thermalTransportModel "Ranz and Marshall formula for convective heat transfer coefficient" ;
                cs:depositionCriterion "Critical Velocity criterion implemented via User Defined Function (UDF)" ;
                cs:meshRefinementTechnique "Mesh independence study conducted across five grids; selection based on air velocity convergence along the nozzle axis" ;
                cs:meshResolution "211,233 tetrahedral elements (basic method); 222,620 tetrahedral elements (novel methods)" ;
                cs:particleBoundaryCondition "20 µm diameter spherical copper particles; initial temperature 298 K; mass flow rate 0.00083 kg/s" ;
                cs:substrateBoundaryCondition "Inlet mass flow 0.07 kg/s; gas temperature 623 K; outlet ambient pressure 0.101 MPa" ;
                cs:contactInteractionMethod "One-way coupling; elastic rebound assumed if impact velocity is below critical velocity" ;

                # 2.3 Model Constants
                cs:hasModelingConstant [
                    a cs:ModelConstant ;
                    cs:parameterName "Copper Density" ;
                    cs:parameterValue "8978" ;
                    cs:parameterUnit "kg/m3"
                ] ;
